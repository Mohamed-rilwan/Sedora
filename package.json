{
  "name": "paper-dashboard-react",
  "version": "1.3.0",
  "private": true,
  "homepage": "https://demos.creative-tim.com/paper-dashboard-react/#/dashboard",
  "dependencies": {
    "@azure/msal-browser": "^2.15.0",
    "@babel/core": "7.14.2",
    "bootstrap": "4.6.0",
    "chart.js": "3.2.1",
    "jquery": "3.6.0",
    "js-export-excel": "^1.1.4",
    "moment": "^2.29.1",
    "node-sass": "6.0.0",
    "normal-distribution": "^1.1.1",
    "perfect-scrollbar": "1.5.1",
    "react": "17.0.2",
    "react-chartjs-2": "3.0.3",
    "react-data-export": "^0.6.0",
    "react-dom": "17.0.2",
    "react-notification-alert": "0.0.13",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "^5.0.1",
    "reactstrap": "8.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.css",
